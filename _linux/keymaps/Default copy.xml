<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="shift alt q" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="shift alt home" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="shift alt end" />
  </action>
  <action id="HungryBackSpace.StopAtIndent">
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="ImportProject">
    <keyboard-shortcut first-keystroke="shift alt p" />
  </action>
  <action id="NewProjectFromVCS">
    <keyboard-shortcut first-keystroke="shift alt g" />
  </action>
  <action id="PinToolwindowTab">
    <keyboard-shortcut first-keystroke="shift alt quote" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="shift alt e" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt enter" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt down" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt left" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt right" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt up" />
  </action>
  <action id="ToggleCamelAction">
    <keyboard-shortcut first-keystroke="ctrl alt quote" />
  </action>
</keymap>